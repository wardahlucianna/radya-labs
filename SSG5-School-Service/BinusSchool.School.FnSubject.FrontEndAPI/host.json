{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Microsoft.EntityFrameworkCore": "Information",
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "school-fn-subject"
    },
    "queues": {
      "maxDequeueCount": 1
    }
  }
}